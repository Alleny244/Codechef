#include <iostream>

using namespace std;

int main() {
  // your code goes here
  int n;
  cin >> n;
  switch (n) {
  case 1...50: {
    cout << 100 << endl;
    break;
  }
  case 51...100: {
    cout << 50 << endl;
    break;
  }
  default: {
    cout << 0 << endl;
    break;
  }
  }
  return 0;
}
