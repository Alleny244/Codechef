class Solution {
public:
    int romanToInt(string s) {
        int sum=0;
        int i=0;

        while(i<s.length()){
            if(s[i]=='I'){
                if(s[i+1]=='V'){
                    sum+=4;
                   i+=2;
                }
                else if(s[i+1]=='X'){
                     sum+=9;
                     i+=2;
                }
                 else if(s[i+1]=='I'){
              sum+=2;
                         i+=2;
                }
                     else{    
                              int value;
           switch(s[i]){
                case 'I':{
                    value=1;
                    break;
                }
                      case 'V':{
                    value=5;
                    break;
                }
              case 'X':{
                    value=10;
                    break;
                }          
                  case 'L':{
                    value=50;
                    break;
                }          
                           
                        case 'C':{
                    value=100;
                    break;
                }          
                            
                           
                        case 'D':{
                    value=500;
                    break;
                }          
                   
                           case 'M':{
                    value=1000;
                    break;
                }         
                    
                    }
                
                sum+=value;
                  i++;
             
            }
            
            }
            
                   else if(s[i]=='X'){
                if(s[i+1]=='L'){
                      sum+=40;
                   i+=2;
                }
                else if(s[i+1]=='C'){
                      sum+=90;
                        i+=2;
                }
                                                                 else{    
                              int value;
           switch(s[i]){
                case 'I':{
                    value=1;
                    break;
                }
                      case 'V':{
                    value=5;
                    break;
                }
              case 'X':{
                    value=10;
                    break;
                }          
                  case 'L':{
                    value=50;
                    break;
                }          
                           
                        case 'C':{
                    value=100;
                    break;
                }          
                            
                           
                        case 'D':{
                    value=500;
                    break;
                }          
                   
                           case 'M':{
                    value=1000;
                    break;
                }         
                    
                    }
                
                sum+=value;
                  i++;
             
            }     
                   }
       
            
                      else if(s[i]=='C'){
                if(s[i+1]=='D'){
                      sum+=400;
                      i+=2;
                }
                else if(s[i+1]=='M'){
                      sum+=900;
                      i+=2;
                }
                                               else{    
                              int value;
           switch(s[i]){
                case 'I':{
                    value=1;
                    break;
                }
                      case 'V':{
                    value=5;
                    break;
                }
              case 'X':{
                    value=10;
                    break;
                }          
                  case 'L':{
                    value=50;
                    break;
                }          
                           
                        case 'C':{
                    value=100;
                    break;
                }          
                            
                           
                        case 'D':{
                    value=500;
                    break;
                }          
                   
                           case 'M':{
                    value=1000;
                    break;
                }         
                    
                    }
                
                sum+=value;
                  i++;
             
            }
       
            }
            else{
                                            int value;
           switch(s[i]){
                case 'I':{
                    value=1;
                    break;
                }
                      case 'V':{
                    value=5;
                    break;
                }
              case 'X':{
                    value=10;
                    break;
                }          
                  case 'L':{
                    value=50;
                    break;
                }          
                           
                        case 'C':{
                    value=100;
                    break;
                }          
                            
                           
                        case 'D':{
                    value=500;
                    break;
                }          
                   
                           case 'M':{
                    value=1000;
                    break;
                }         
                    
                    }
                
                sum+=value;
                  i++;
             
            }
       
            }
         
                


        
          
        
        return sum;
        
    }
};
